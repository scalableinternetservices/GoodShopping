<div class="good-shopping-banner-container">
  <div class="good-shopping-banner-logo"></div>
  <p class="good-shopping-banner-text">Good Shopping</p>
</div>



<h1>New Shopper</h1>

<%= simple_form_for @shopper do |f| %>

  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :username %>
  <%= f.input :password %>
  <%= f.input :email %>
  <%= f.input :address %>
  <%= f.input :telephone %>


 <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>
